<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title>@ViewData["Title"] Meetmax</title>
    <link rel="stylesheet" href="~/css/global.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@500;700;900&display=swap" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

</head>
<body>
    <header class="cardwhiteheader">
        <div class="background1"></div>
        <div class="header-logo">
            <div class="iconlogologo">
                <div class="logo">
                    <div class="logo-container">
                        <img class="logo-icon"
                             loading="lazy"
                             alt=""
                             src="~/Icon/Meetmax.png" />
                    </div>
                    <a class="meetmax">Meetmax</a>
                </div>
            </div>
        </div>
        <div class="select-language">
            <div class="bg"></div>
            <a class="english-uk">English (UK)</a>
            <div class="dropdown">
                <img class="iconfillangle-down"
                     alt=""
                     src="~/Icon/Angle-down.png" />
            </div>
        </div>
    </header>

    <div class="main-content">
        @RenderBody()
    </div>

    @RenderSection("Scripts", required: false)
</body>
</html>